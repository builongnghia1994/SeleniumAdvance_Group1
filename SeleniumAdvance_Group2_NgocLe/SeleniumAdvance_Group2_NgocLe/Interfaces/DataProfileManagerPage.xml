<?xml version="1.0" encoding="utf-8" ?>
<DataProfileManager>
  <tblDataProfile>
    <name>data profile table</name>
    <type>xpath</type>
    <value>//table[@class='GridView']/tbody/tr</value>
  </tblDataProfile>
  <linkAddNew>
    <name>add new link</name>
    <type>xpath</type>
    <value>//form[@id='form1']//a[@href='profile.jsp?action=create']</value>
  </linkAddNew>
  <cbCheckAll>
    <name>check all checkbox</name>
    <type>xpath</type>
    <value>//a[text()='Check All']</value>
  </cbCheckAll>
  <linkDelete>
    <name>delete link</name>
    <type>xpath</type>
    <value>//div[@class='panel_tag2']/a[text()='Delete']</value>
  </linkDelete>
</DataProfileManager>