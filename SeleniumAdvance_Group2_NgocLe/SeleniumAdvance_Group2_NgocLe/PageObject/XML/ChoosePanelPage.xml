<?xml version="1.0" encoding="utf-8" ?>
<ChoosePanel>
  <btnCreateNewPanel>
    <name>create new panel button</name>
    <type>xpath</type>
    <value>//span[text()='Create new panel']</value>
  </btnCreateNewPanel>
  <btnHide>
    <name>hide button</name>
    <type>id</type>
    <value>btnHidePanel</value>
  </btnHide>
  <itemsColumnChart>
    <name>items column in chart</name>
    <type>xpath</type>
    <value>//div[@id='container']//div[@class='ptit pchart']/../table//tr[1]/td</value>
  </itemsColumnChart>
  <itemsRowChart>
    <name>items row in chart</name>
    <type>xpath</type>
    <value>//div[@id='container']//div[@class='ptit pchart']/../table//tr</value>
  </itemsRowChart>
</ChoosePanel>